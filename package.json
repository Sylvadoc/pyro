{
  "name": "pyro",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite-ssr --port 1337",
    "dev:spa": "vite --port 1337",
    "clean": "rm -rf dist && rm -rf serverless/public && rm -rf serverless/api/renderer",
    "build": "pnpm clean && cross-env NODE_ENV=production vite-ssr build && ./scripts/move-dist.sh"
  },
  "dependencies": {
    "@vueuse/core": "^6.7.6",
    "@vueuse/head": "^0.7.5",
    "nprogress": "^0.2.0",
    "vite-ssr": "^0.15.0",
    "vue": "^3.2.29",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@types/nprogress": "^0.2.0",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/server-renderer": "^3.2.29",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "node-fetch": "^3.2.0",
    "pnpm": "^6.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "unplugin-vue-components": "^0.17.15",
    "vite-plugin-pages": "^0.20.0",
    "vite-plugin-vue-layouts": "^0.5.0"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}